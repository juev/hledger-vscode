{
  "comments": {
    "lineComment": ";"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*comment\\s*$",
      "end": "^\\s*end\\s+comment\\s*$"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(account|comment|apply\\s+account)\\s+.*$",
    "decreaseIndentPattern": "^\\s*(end\\s+comment|end\\s+apply\\s+account)\\s*$",
    "indentNextLinePattern": "^(\\d{4}[-/]\\d{1,2}[-/]\\d{1,2}|\\d{1,2}[-/]\\d{1,2}).*$"
  },
  "onEnterRules": [
    {
      "beforeText": "^(\\d{4}[-/]\\d{1,2}[-/]\\d{1,2}|\\d{1,2}[-/]\\d{1,2})\\s*(\\*|!)?\\s*(\\([^)]+\\))?\\s*.*$",
      "action": {
        "indent": "indent"
      }
    }
  ],
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\?\\/\\s]+)"
}