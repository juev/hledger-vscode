{
  "name": "hledger-vscode",
  "displayName": "hledger",
  "description": "Syntax highlighting and IntelliSense support for hledger journal files",
  "version": "0.1.0",
  "publisher": "hledger-vscode",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "hledger",
    "accounting",
    "finance",
    "ledger"
  ],
  "main": "./out/extension/main.js",
  "activationEvents": [
    "onLanguage:hledger"
  ],
  "contributes": {
    "languages": [
      {
        "id": "hledger",
        "aliases": [
          "hledger",
          "HLedger"
        ],
        "extensions": [
          ".journal",
          ".hledger",
          ".ledger"
        ],
        "filenames": [
          "journal",
          ".journal"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "hledger",
        "scopeName": "source.hledger",
        "path": "./syntaxes/hledger.tmLanguage.json"
      }
    ],
    "configurationDefaults": {
      "files.associations": {
        "*.journal": "hledger",
        "*.hledger": "hledger",
        "*.ledger": "hledger",
        "journal": "hledger"
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "glob": "^8.0.3"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/juev/hledger-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/juev/hledger-vscode/issues"
  },
  "homepage": "https://github.com/juev/hledger-vscode#readme"
}