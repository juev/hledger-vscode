{
  "name": "hledger Light",
  "type": "light",
  "colors": {},
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment.line.semicolon.hledger",
        "comment.line.number-sign.hledger",
        "comment.line.asterisk.hledger"
      ],
      "settings": {
        "foreground": "#008000"
      }
    },
    {
      "name": "Directive",
      "scope": [
        "keyword.control.directive.hledger"
      ],
      "settings": {
        "foreground": "#AF00DB",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Date",
      "scope": [
        "constant.numeric.date.hledger",
        "constant.numeric.date.auxiliary.hledger"
      ],
      "settings": {
        "foreground": "#098658"
      }
    },
    {
      "name": "Transaction Status",
      "scope": [
        "keyword.transaction.status.hledger"
      ],
      "settings": {
        "foreground": "#0000FF"
      }
    },
    {
      "name": "Transaction Code",
      "scope": [
        "entity.name.tag.transaction-code.hledger"
      ],
      "settings": {
        "foreground": "#267F99"
      }
    },
    {
      "name": "Description",
      "scope": [
        "string.unquoted.description.hledger"
      ],
      "settings": {
        "foreground": "#A31515"
      }
    },
    {
      "name": "Account",
      "scope": [
        "entity.name.account.hledger"
      ],
      "settings": {
        "foreground": "#001080"
      }
    },
    {
      "name": "Amount",
      "scope": [
        "constant.numeric.hledger"
      ],
      "settings": {
        "foreground": "#098658"
      }
    },
    {
      "name": "Commodity",
      "scope": [
        "string.commodity.hledger"
      ],
      "settings": {
        "foreground": "#795E26"
      }
    },
    {
      "name": "Assertion Operator",
      "scope": [
        "keyword.operator.hledger"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Filepath",
      "scope": [
        "string.unquoted.filepath.hledger"
      ],
      "settings": {
        "foreground": "#A31515"
      }
    }
  ]
}